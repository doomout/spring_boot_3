{{>layouts/header}}
<!--상세 목록 페이지 -->
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#member}} <!--모델에 등록되어 있는 데이터를 출력-->
    <tr>
        <th>{{id}}</th>
        <td>{{email}}</td>
        <td>{{password}}</td>
    </tr>
    {{/member}}
    </tbody>
</table>
<a href="/members/{{member.id}}/edit" class="btn btn-primary">수정하기</a> <!--수정 페이지로 이동-->
<a href="/members/{{member.id}}/delete" class="btn btn-primary">삭제하기</a><!--삭제 페이지로 이동-->
<a href="/members">목록</a> <!--목폭 페이지로 이동-->
{{>layouts/footer}}
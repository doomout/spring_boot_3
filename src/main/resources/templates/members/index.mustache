{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#members}} <!--모델에 등록되어 있는 데이터를 출력-->
    <tr>
        <th>{{id}}</th>
        <td><a href="/members/{{id}}">{{email}}</a></td>
        <td>{{password}}</td>
    </tr>
    {{/members}}
    </tbody>
</table>

<a href="/members/new">회원가입</a>
{{>layouts/footer}}